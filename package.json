{
  "name": "arcads",
  "version": "1.4.5",
  "description": "ArcAds is a DFP wrapper created by Arc Publishing with publishers in mind.",
  "main": "dist/arcads.node.js",
  "files": [
    "dist/*.js"
  ],
  "unpkg": "dist/arcads.js",
  "scripts": {
    "test": "jest --config jest.config.js --no-cache",
    "build": "rm -rf dist && webpack --mode=production",
    "dev": "rm -rf dist && webpack --mode=development --watch",
    "docs": "rm -rf docs && ./node_modules/.bin/esdoc && open docs/index.html",
    "debug": "node debugging.js",
    "lint": "eslint --ext js ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/washingtonpost/arcads.git"
  },
  "keywords": [
    "ads",
    "advertisements",
    "dfp",
    "gpt",
    "prebid",
    "amazon",
    "headerbidding",
    "hb"
  ],
  "author": "Arc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/washingtonpost/arcads/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-root-import": "^6.4.1",
    "core-js": "^2.6.9",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.6.0"
  },
  "homepage": "https://github.com/washingtonpost/arcads#readme"
}
